{
  "OpenCV": {
    "ModelsPath": "./models",
    "FaceDetection": {
      "Model": "res10_ssd_deploy.prototxt",
      "Weights": "res10_300x300_ssd_iter_140000.caffemodel",
      "ConfidenceThreshold": 0.7
    },
    "YOLOv8": {
      "ModelPath": "./models/yolov8-face.onnx",
      "ConfidenceThreshold": 0.6
    },
    "HaarCascades": {
      "FaceCascade": "haarcascade_frontalface_default.xml",
      "EyeCascade": "haarcascade_eye.xml",
      "LeftEyeCascade": "haarcascade_lefteye_2splits.xml",
      "RightEyeCascade": "haarcascade_righteye_2splits.xml"
    }
  },
  "Ollama": {
    "BaseUrl": "http://localhost:11434",
    "NameMatching": {
      "Model": "qwen2.5:7b",
      "ConfidenceThreshold": 0.9
    },
    "FaceDetection": {
      "Model": "qwen3-vl"
    }
  },
  "ImageProcessing": {
    "PortraitWidth": 800,
    "PortraitHeight": 1000,
    "SupportedFormats": [ "jpg", "jpeg", "png", "bmp" ]
  },
  "Benchmark": {
    "Iterations": 10,
    "WarmupIterations": 3
  }
}
